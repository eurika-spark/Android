<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    	xmlns:tools="http://schemas.android.com/tools"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical" 
	    >

	<TextView
	    android:id="@+id/page_2_print"
	    android:layout_width="110dp"
	    android:layout_height="150dp"
	    android:layout_alignParentTop="true"
	    android:layout_alignParentLeft="true"
	    android:text="@string/toPrintText" 
	    style="@style/main_textBlock_orange"
	    />
	    
	<TextView
	    android:id="@+id/page_2_listView"
	    android:layout_width="120dp"
	    android:layout_height="80dp"
	    android:layout_toRightOf="@id/page_2_print"
	    android:layout_alignTop="@id/page_2_print"
	    android:text="@string/toListView" 
	    style="@style/main_textBlock_blue"
	    />
    
    
</RelativeLayout>
    
    
